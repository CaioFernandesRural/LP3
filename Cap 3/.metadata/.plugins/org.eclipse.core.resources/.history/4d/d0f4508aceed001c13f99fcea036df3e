
public class Invoice 
{
	private String numero;
	private String descricao;
	private int quantia;
	private double preco;
	
	public Invoice(String numero, String descricao, int quantia, double preco)
	{
		this.numero = numero;
		this.descricao = descricao;
		this.quantia = quantia;
		if (preco > 0.0)
			this.preco = preco;
	}
	
}
